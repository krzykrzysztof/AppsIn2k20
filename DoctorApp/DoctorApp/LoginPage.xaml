<Window x:Class="DoctorApp.LoginPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoctorApp"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="337.538" Background="Cornsilk">
    <Grid>
        <Button x:Name="SingInBnt" Content="Sing In" Margin="0,312,0,0" Click="SingInBnt_Click"/>
        <Label Content="Login" Margin="10,52,158,272"/>
        <Label Content="Password" Margin="10,152,40,220"/>
        <TextBox x:Name="Loginbox"  Margin="80,48,106,337"/>
        <PasswordBox x:Name="PasswordBox" Margin="80,152,106,233"/>
    </Grid>
</Window>
